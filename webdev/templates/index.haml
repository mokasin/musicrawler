      %div.navbar-inner
        %div.container
          %a.btn.btn-navbar(data-toggle="collapse" data-target=".nav-collapse")
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.brand(href="#") musicrawler
          %div.nav-collapse
            %ul.nav
              %li.active
                %a(href="#") Home
              %li
                %a(href="#about") About

    %div.container
      {{.Body}}
    / Placed at the end of the document so the pages load faster
    %script(src="assets/js/SoundManager2/soundmanager2-nodebug-jsmin.js")
